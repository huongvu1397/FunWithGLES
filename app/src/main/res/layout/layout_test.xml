<?xml version="1.0" encoding="utf-8"?>
<com.excalibur.funwithgles.catchpack.CanTouchView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rootview"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <com.excalibur.funwithgles.catchpack.ConstRelativeLayout
        android:id="@+id/cr"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:id="@+id/adrl"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:visibility="gone" />

        <RelativeLayout
            android:id="@+id/countrl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/adrl"
            android:layout_centerHorizontal="true">

            <TextureView
                android:id="@+id/surface"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true" />

            <LinearLayout
                android:id="@+id/errorll"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:orientation="vertical"
                android:visibility="gone">

                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    />

                <TextView
                    android:id="@+id/errortv"
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="center"
                    android:textColor="#b3b3b3"
                    android:textSize="14dp"
                    android:visibility="visible" />
            </LinearLayout>

<!--            <photoeffect.photomusic.slideshow.baselibs.sticker.View.MyStickerCanvasView-->
<!--                android:id="@+id/stickerview"-->
<!--                android:layout_width="wrap_content"-->
<!--                android:layout_height="wrap_content"-->
<!--                android:layout_centerInParent="true"-->
<!--                android:visibility="gone" />-->

            <View
                android:visibility="gone"
                android:id="@+id/stickerview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

<!--            <photoeffect.photomusic.slideshow.basecontent.View.ShowCenterLineView-->
<!--                android:id="@+id/showline"-->
<!--                android:layout_width="match_parent"-->
<!--                android:layout_height="match_parent"-->
<!--                android:visibility="gone" />-->

            <ImageView
                android:id="@+id/watermark"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:layout_alignRight="@+id/watermarkshow"
                android:layout_alignBottom="@+id/stickerview"
                android:layout_marginBottom="30dp"
                android:visibility="gone" />

<!--            <photoeffect.photomusic.slideshow.basecontent.View.watermark.WaterMarkImageView-->
<!--                android:id="@+id/watermarkshow"-->
<!--                android:layout_width="69dp"-->
<!--                android:layout_height="26dp"-->
<!--                android:layout_alignRight="@+id/stickerview"-->
<!--                android:layout_alignBottom="@+id/stickerview"-->
<!--                android:layout_marginRight="@dimen/size11"-->
<!--                android:layout_marginBottom="@dimen/size5" />-->
            <View
                            android:id="@+id/watermarkshow"
                android:layout_width="69dp"
                android:layout_height="26dp"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/toprl"
            android:layout_width="match_parent"
            android:layout_height="50dp">

            <FrameLayout
                android:id="@+id/btn_back"
                android:layout_width="50dp"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/backiv"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:layout_gravity="center"
                    />
            </FrameLayout>

            <TextView
                android:id="@+id/showtv"
                android:layout_centerInParent="true"
                android:text="Edit"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:orientation="horizontal">

                <FrameLayout
                    android:id="@+id/btn_look"
                    android:layout_width="50dp"
                    android:layout_height="match_parent"
                    android:visibility="visible">

                    <ImageView
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        android:layout_gravity="center" />
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/btn_go"
                    android:layout_width="50dp"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/saveiv"
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        android:layout_gravity="center"
                        />
                </FrameLayout>
            </LinearLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/bottommenu"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/countrl"
            android:layout_alignParentBottom="true">

            <RelativeLayout
                android:id="@+id/menu"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="#141414">

                <LinearLayout
                    android:id="@+id/hisll"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/hisbackiv"
                        android:layout_width="22dp"
                        android:layout_height="22dp"
                        android:layout_centerVertical="true"
                        android:visibility="visible" />

                    <ImageView
                        android:id="@+id/hisgoiv"
                        android:layout_width="22dp"
                        android:layout_height="22dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:visibility="visible" />

                    <TextView
                        android:id="@+id/hisnum"
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:layout_toRightOf="@+id/hisgoiv"
                        android:gravity="center"
                        android:textSize="12dp" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/playbt"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_centerInParent="true"
                    android:padding="4dp"
                    />

                <ImageView
                    android:id="@+id/effectunlk"
                    android:layout_width="54dp"
                    android:layout_height="26dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="50dp"
                    android:layout_marginRight="50dp"
                    android:visibility="gone" />

                <LinearLayout
                    android:id="@+id/timell"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/timetv1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="00:00"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/timec"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="2dp"
                        android:text="/"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/timetv2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="00:06"
                        android:textSize="10dp" />
                </LinearLayout>
            </RelativeLayout>

<!--            <photoeffect.photomusic.slideshow.basecontent.View.adjustprogress.AdjustProgressBar-->
<!--                android:id="@+id/myadjustbar"-->
<!--                android:layout_width="match_parent"-->
<!--                android:layout_height="match_parent"-->
<!--                android:layout_above="@+id/bottomparent"-->
<!--                android:layout_below="@+id/menu" />-->

            <RelativeLayout
                android:id="@+id/bottomparent"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true">

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/myrec"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/delrl"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_alignParentBottom="true" />

            <RelativeLayout
                android:id="@+id/volumeparent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="60dp" />

            <RelativeLayout
                android:id="@+id/effectparent"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/menu" />

            <RelativeLayout
                android:id="@+id/watermarkparent"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/menu" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/tishirl"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#88000000"
            android:visibility="gone">

            <LinearLayout
                android:id="@+id/tishill"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingStart="10dp"
                android:paddingLeft="10dp"
                android:visibility="visible">

                <TextView
                    android:id="@+id/tishitv"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:gravity="center"
                    android:paddingStart="16dp"
                    android:paddingLeft="16dp"
                    android:paddingEnd="16dp"
                    android:paddingRight="16dp"
                    android:text="trantishi"
                    android:textColor="#000000"
                    android:textSize="16dp" />
            </LinearLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/showrl"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#000"
            android:visibility="gone">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_alignParentBottom="true"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/showplaybt"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    />

                <TextView
                    android:id="@+id/showtv1"
                    android:layout_width="32dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="10dp"
                    android:text="00:00"
                    android:textSize="10dp" />

                <SeekBar
                    android:id="@+id/showsk"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:thumbOffset="6dp"
                    />

                <TextView
                    android:id="@+id/showtv2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="10dp"
                    android:layout_marginRight="10dp"
                    android:text="00:00"
                    android:textSize="10dp" />

                <ImageView
                    android:id="@+id/showclosebt"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginRight="16dp"
                    />
            </LinearLayout>
        </RelativeLayout>

        <View
            android:id="@+id/mask"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#99000000"
            android:visibility="gone" />
    </com.excalibur.funwithgles.catchpack.ConstRelativeLayout>

    <com.excalibur.funwithgles.catchpack.GuideView
        android:id="@+id/guideview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />
</com.excalibur.funwithgles.catchpack.CanTouchView>